{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "graphql": true
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "react/jsx-wrap-multilines": 0,
    "react/no-danger": 0,
    "react/prop-types": 0,
    "no-unexpected-multiline": 2
  }
}
