{
  "name": "learn-islam",
  "description": "A clone of udacity to learn Islam",
  "version": "0.0.1",
  "author": "abumalick",
  "license": "Unlicense",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "eval $(cat .env.prod) gatsby build",
    "clean": "rimraf ./public",
    "commit": "git-cz",
    "full-build": "yarn build-plugin; yarn build",
    "build-plugin":
      "cd plugins/gatsby-source-rest/; yarn; yarn build:prod; cd ../../",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "stylelint --syntax=scss ./src/**/*.scss",
    "lint:js": "eslint .",
    "no-precommit": "lint-staged",
    "preview": "npm run clean && npm run build && yarn serve",
    "serve": "http-server ./public -p 8000 -P http://localhost:8000",
    "start": "npm run start:prod",
    "start:dev": "eval $(cat .env.dev) gatsby develop -H localhost",
    "start:prod": "eval $(cat .env.prod) gatsby develop -H localhost",
    "test:security": "nsp check"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --ignore-pattern '!.eslintrc.js'",
      "git add"
    ],
    "*.json": ["prettier --write", "git add"],
    "*.css": ["prettier --write", "git add"],
    "*.scss": ["prettier --write", "git add"],
    "*.{md}": ["prettier --write", "git add", "git add"],
    "*.{png,jpeg,jpg,gif,svg}": ["imagemin-lint-staged", "git add"]
  },
  "dependencies": {
    "@material/button": "^0.35.0",
    "@material/card": "^0.35.0",
    "@material/drawer": "^0.35.0",
    "@material/form-field": "^0.35.0",
    "@material/list": "^0.35.0",
    "@material/radio": "^0.35.0",
    "@material/select": "^0.35.2",
    "@material/textfield": "^0.35.1",
    "@material/theme": "^0.35.0",
    "@material/toolbar": "^0.35.0",
    "@material/typography": "^0.35.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-flow-react-proptypes": "^24.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "classnames": "^2.2.5",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "flow-bin": "^0.72.0",
    "gatsby": "^1.9.261",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-catch-links": "^1.0.22",
    "gatsby-plugin-emotion": "^1.1.16",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-mailchimp": "^2.2.3",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-postcss-sass": "^1.0.20",
    "gatsby-plugin-preact": "^1.0.17",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-resolve-src": "^1.1.3",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sentry": "^0.0.4",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-webpack-bundle-analyzer": "^0.1.1",
    "make-plural": "^4.2.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "rimraf": "^2.6.1",
    "slash": "^2.0.0",
    "tachyons": "^4.9.1"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.1.1",
    "nsp": "^3.2.1",
    "prettier": "^1.12.1",
    "stylelint": "^9.2.1",
    "stylelint-config-primer": "^2.2.6"
  }
}
