{
  "name": "learn-islam",
  "description": "A clone of udacity to learn Islam",
  "version": "0.0.1",
  "author": "abumalick",
  "license": "Unlicense",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "eval $(cat .env.prod) gatsby build",
    "clean": "rimraf ./public",
    "commit": "git-cz",
    "full-build": "yarn build-plugin; yarn build",
    "build-plugin": "cd plugins/gatsby-source-rest/; yarn; yarn build:prod; cd ../../",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "stylelint --syntax=scss ./src/**/*.scss",
    "lint:js": "eslint .",
    "no-precommit": "lint-staged",
    "preview": "npm run clean && npm run build && yarn serve",
    "serve": "http-server ./public -p 8000 -P http://localhost:8000",
    "start": "npm run start:prod",
    "start:dev": "eval $(cat .env.dev) gatsby develop",
    "start:prod": "eval $(cat .env.prod) gatsby develop",
    "test:security": "nsp check"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --ignore-pattern '!.eslintrc.js'",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@material/button": "^0.35.0",
    "@material/card": "^0.35.0",
    "@material/drawer": "^0.35.0",
    "@material/form-field": "^0.35.0",
    "@material/list": "^0.35.0",
    "@material/radio": "^0.35.0",
    "@material/select": "^0.35.2",
    "@material/textfield": "^0.35.1",
    "@material/theme": "^0.35.0",
    "@material/toolbar": "^0.35.0",
    "@material/typography": "^0.35.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-flow-react-proptypes": "^24.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "classnames": "^2.2.6",
    "flow-bin": "^0.76.0",
    "gatsby": "^1.9.273",
    "gatsby-image": "^1.0.54",
    "gatsby-link": "^1.6.45",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-catch-links": "^1.0.24",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-mailchimp": "^2.2.3",
    "gatsby-plugin-manifest": "^1.0.27",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-postcss-sass": "^1.0.22",
    "gatsby-plugin-preact": "^1.0.17",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-resolve-src": "^1.1.3",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sentry": "^0.1.0",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-webpack-bundle-analyzer": "^0.1.1",
    "make-plural": "^4.2.0",
    "memoize-one": "^4.0.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.1",
    "scroll-into-view-if-needed": "^2.2.11",
    "slash": "^2.0.0",
    "svg-icon": "^0.8.2",
    "tachyons": "^4.10.0",
    "typeface-amiri": "^0.0.56",
    "typeface-open-sans": "^0.0.54"
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.2.0",
    "nsp": "^3.2.1",
    "prettier": "^1.13.7",
    "stylelint": "^9.3.0",
    "stylelint-config-primer": "^2.2.9"
  }
}
