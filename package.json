{
  "name": "learn-islam",
  "description": "A clone of udacity to learn Islam",
  "version": "0.0.1",
  "author": "abumalick",
  "license": "Unlicense",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "eval $(cat .env.prod) gatsby build",
    "clean": "rimraf public .cache node_modules/.bin",
    "commit": "git-cz",
    "full-build": "yarn build-plugin; yarn build",
    "build-plugin": "cd plugins/gatsby-source-rest/; yarn; yarn build:prod; cd ../../",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "stylelint --syntax=scss ./src/**/*.scss",
    "lint:js": "eslint .",
    "no-precommit": "lint-staged",
    "preview": "npm run clean && npm run build && yarn serve",
    "serve": "http-server ./public -p 8000 -P http://localhost:8000",
    "start": "npm run start:prod",
    "start:dev": "eval $(cat .env.dev) gatsby develop",
    "start:prod": "eval $(cat .env.prod) gatsby develop",
    "test:security": "nsp check",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --ignore-pattern '!.eslintrc.js'",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@material/button": "^0.35.0",
    "@material/card": "^0.35.0",
    "@material/checkbox": "^0.37.1",
    "@material/drawer": "^0.35.0",
    "@material/form-field": "^0.37.1",
    "@material/list": "^0.35.0",
    "@material/radio": "^0.35.0",
    "@material/select": "^0.35.2",
    "@material/textfield": "^0.35.1",
    "@material/theme": "^0.35.0",
    "@material/toolbar": "^0.35.0",
    "@material/typography": "^0.35.0",
    "@sentry/browser": "^4.5.3",
    "@types/classnames": "^2.2.7",
    "@types/ramda": "^0.25.47",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-icons": "^2.2.6",
    "@types/react-redux": "^7.0.1",
    "@types/recompose": "^0.30.3",
    "babel-eslint": "^10.0.1",
    "caniuse-lite": "^1.0.30000934",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.5",
    "gatsby": "^2.0.111",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-canonical-urls": "^2.0.10",
    "gatsby-plugin-catch-links": "^2.0.10",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-mailchimp": "https://github.com/benjaminhoffman/gatsby-plugin-mailchimp.git#gatsby-v2",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-netlify": "^2.0.8",
    "gatsby-plugin-nprogress": "^2.0.8",
    "gatsby-plugin-offline": "^2.0.22",
    "gatsby-plugin-postcss": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sitemap": "^2.0.5",
    "gatsby-plugin-typescript": "^2.0.6",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-source-graphql": "^2.0.10",
    "gatsby-transformer-json": "^2.1.8",
    "localforage": "^1.7.3",
    "make-plural": "^4.3.0",
    "memoize-one": "^5.0.0",
    "node-sass": "^4.11.0",
