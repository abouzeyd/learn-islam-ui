{
  "name": "gatsby-source-rest",
  "version": "0.0.2",
  "description": "A gatsby source plugin to fetch from REST endpoints",
  "author": "abumalick",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin"],
  "license": "MIT",
  "main": "index.js",
  "homepage":
    "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-rest#readme",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "ramda": "^0.25.0",
    "underscore.string.fp": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.3"
  }
}
