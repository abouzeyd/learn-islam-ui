{
  "name": "gatsby-source-rest",
  "version": "0.0.2",
  "description": "A gatsby source plugin to fetch from REST endpoints",
  "author": "abumalick",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-rest#readme",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "axios": "^0.18.0",
    "ramda": "^0.25.0",
    "underscore.string.fp": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "cross-env": "^5.2.0"
  }
}
